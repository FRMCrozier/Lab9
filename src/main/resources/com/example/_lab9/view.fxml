<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="540.0" prefWidth="700.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example._lab9.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
      <Label prefHeight="18.0" prefWidth="66.0" text="Search Box:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="filterField" promptText="add filter" />
        <Label layoutX="14.0" layoutY="18.0" text="Cylinders info:" />
        <TableView id="tableCylinders" fx:id="tableCylinders" editable="true" prefHeight="241.0" prefWidth="351.0">
            <columns>
                <TableColumn id="height" fx:id="height" prefWidth="130.0" text="Height" />
                <TableColumn id="radius" fx:id="radius" prefWidth="130.0" text="Radius" />
                <TableColumn id="square" fx:id="square" prefWidth="130.0" text="Square" />
                <TableColumn id="circumference" fx:id="circumference" prefWidth="130.0" text="Circumference" />
                <TableColumn id="volume" fx:id="volume" prefWidth="130.0" text="Volume" />
            </columns>
        </TableView>
    </children>
</VBox>
